<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.libo.yibai.MainActivity">

    <Button
        android:id="@+id/btn_TestActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="start Service"
        />

    <Button
        android:id="@+id/btn_broadcast_receiver"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/btn_TestActivity"
        android:text="广播"
        />

    <Button
        android:id="@+id/btn_providerund"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/btn_broadcast_receiver"
        android:text="内容提供者"
        />
</android.support.constraint.ConstraintLayout>